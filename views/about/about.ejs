                                <div class="deal-title">200</div>
                                <div class="deal-description">Different vehicles</div>
                            </div>
                        </div>
                        <div class="deal">
                            <i class="deal-img fa-duotone fa-garage-car fa-8x" style="--fa-primary-color: #FF4C30; --fa-secondary-color: #FF4C30;"></i>
                            <div class="deal-details">
                                <div class="deal-title">5</div>
                                <div class="deal-description">Rental Outlets across India</div>
                            </div>
                        </div>
                        <div class="deal">
